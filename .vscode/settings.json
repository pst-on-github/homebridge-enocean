{
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.rulers": [
    140
  ],
  "eslint.enable": true,

  "[json]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
},
"cSpell.words": [
    "eltaco",
    "ELTAKO",
    "enocean",
    "fakegato",
    "rorg",
    "rssi"
]
}
